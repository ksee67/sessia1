<Application x:Class="DStore.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:DStore"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <FontFamily x:Key="MainFontFamilyApp">Comic Sans MS</FontFamily>
        <SolidColorBrush x:Key="MainColorApp" Color="#fff"/>
        <SolidColorBrush x:Key="AdditionalColorApp" Color="#92d050"/>
        <SolidColorBrush x:Key="FocusColorApp" Color="#349c3d"/>
    </Application.Resources>
</Application>
